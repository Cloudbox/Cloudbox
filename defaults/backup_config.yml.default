---
local:
  enable: yes
  destination: /home/{{ user }}/Backups/Cloudbox
rclone:
  enable: yes
  destination: google:/Backups/Cloudbox
rsync:
  enable: no
  dest: rsync://somehost.com/Backups/Cloudbox
cron:
  cron_time: weekly
  cron_state: absent
restore_service:
  user:
  pass:
misc:
  snapshot: true
